{
  "ignorePatterns": [
    {
      "pattern": "^https://hooks.slack.com"
    },
    {
      "pattern": "^https://github.com/Copubah"
    },
    {
      "pattern": "^https://www.linkedin.com/in/charles-opuba-94820574/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}